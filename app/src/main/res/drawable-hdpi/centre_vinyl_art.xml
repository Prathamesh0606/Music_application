<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <size android:height="20dp"
                android:width="20dp"/>
            <solid android:color="#fff"/>
        </shape>
    </item>

    <item android:top="1dp" android:bottom="1dp" android:start="1dp" android:end="1dp">
        <shape android:shape="oval">
            <size android:height="20dp"
                android:width="20dp"/>
            <solid android:color="#fff"/>
        </shape>
    </item>

    <item android:top="4dp" android:bottom="4dp" android:start="4dp" android:end="4dp" >
        <shape android:shape="oval">
            <size android:width="10dp"
                android:height="10dp"/>
            <solid android:color="#fff"/>
        </shape>
    </item>

    <item android:top="11dp" android:bottom="11dp" android:start="11dp" android:end="11dp" >
        <shape android:shape="oval">
            <size android:width="10dp"
                android:height="10dp"/>
            <solid android:color="#000"/>
        </shape>
    </item>

</layer-list>